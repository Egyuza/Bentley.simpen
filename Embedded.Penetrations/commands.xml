<?xml version="1.0" encoding="utf-8" ?>
<KeyinTree xmlns="http://www.bentley.com/schemas/1.0/MicroStation/AddIn/KeyinTree.xsd">

    <RootKeyinTable ID="root">
        <Keyword SubtableRef="Commands" CommandClass="MacroCommand" CommandWord="EMBEDDED">
            <Options Required="true"/>
        </Keyword>
    </RootKeyinTable>

    <SubKeyinTables>
        <KeyinTable ID="Commands">
            <Keyword CommandWord="PENETRATIONS"/>       
        </KeyinTable>   
    </SubKeyinTables>
    
    <KeyinHandlers>
        <KeyinHandler Keyin="EMBEDDED PENETR" Function="Embedded.Penetrations.Keyins.showForm"/>
    </KeyinHandlers>

</KeyinTree>